{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "File Storage",
    "description" : "Storing information for students in a text file",
    "termsOfService" : "https://canvas.qub.ac.uk/courses/24419",
    "contact" : {
      "email" : "d.athanasopoulos@qub.ac.uk"
    },
    "license" : {
      "name" : "Teaching License",
      "url" : "https://canvas.qub.ac.uk/courses/24419"
    },
    "version" : "0.0.1-SNAPSHOT"
  },
  "servers" : [ {
    "url" : "http://localhost:8081"
  } ],
  "paths" : {
    "/Students/enrol" : {
      "post" : {
        "operationId" : "enrol",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/ModuleRequest"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/Students/register" : {
      "post" : {
        "operationId" : "register",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/RegisterRequest"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Response" : {
        "type" : "object",
        "properties" : {
          "code" : {
            "type" : "integer",
            "format" : "int32"
          },
          "responseCode" : {
            "type" : "integer",
            "format" : "int32"
          }
        }
      },
      "ModuleRequest" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "integer",
            "format" : "int32",
            "writeOnly" : true
          },
          "moduleCode" : {
            "type" : "string",
            "writeOnly" : true,
            "enum" : [ "CSC1022", "CSC1023", "CSC1024", "CSC1025", "CSC1026", "CSC1027", "CSC1028", "CSC1029", "CSC1030", "CSC1031" ]
          }
        }
      },
      "RegisterRequest" : {
        "type" : "object",
        "properties" : {
          "studentID" : {
            "type" : "integer",
            "format" : "int32",
            "writeOnly" : true
          }
        }
      }
    }
  }
}